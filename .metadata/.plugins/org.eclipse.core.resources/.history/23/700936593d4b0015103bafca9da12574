class Robot {
	public void speak(String text) {
		System.out.println(text);
	}
	
	public void jump(int height) {
		System.out.println("Jumping: " + height);
	}
}


public class Application {
	public static void main(String[] args) {
		Robot sam = new Robot();
		
		sam.speak("Hello, I'm Sam.");
		sam.jump(7);
	}
}
