
public class Application {

	public static void main(String[] args) {
		
		int value = 7;
		
		int[] values;
		values = new int[3]; // amount of memory allocated is enough to hold 3 integers
		
		System.out.println(values[0]);
		
		values[0] = 10;
		System.out.println(values[0]);
		System.out.println(values[1]);
	}

}
