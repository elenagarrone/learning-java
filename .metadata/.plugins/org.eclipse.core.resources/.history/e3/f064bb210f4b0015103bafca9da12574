
public class Application {
	public static void main(String[] args) {
		int myInt = 7;
		
		String text = "Hello";
		
		String blank = " ";
		
		String name = "Elena";
		
		String greeting = text + blank + name;
		
		System.out.println(text);
		System.out.println(greeting);
		System.out.println("Hello" + " " + "Elena");
	}
}
