class Person {
	String name;
	int age;
	
	void speak() {
		System.out.println("My name is: " + name);
	}
	
	void calculateYearsToRetirement() {
		int yearsLeft = 65 - age;
		
		System.out.println(yearsLeft);
	}
}


public class Application {
	public static void main(String[] args) {
		Person person1 = new Person();
		
		person1.name =  "Elena";
		person1.age = 26;
		person1.speak();
	}
}
