class Frog {
	
	private int id;
	private String name;
	
	public Frog(int id, String name) {
		this.id = id;
		this.name = name;
	}
	
	public String toString() {
		return id + ": " + name;
	}
}


public class Application {
	public static void main(String[] args) {
		Frog frog1 = new Frog();
		
		System.out.println(frog1);
		
	}
}
